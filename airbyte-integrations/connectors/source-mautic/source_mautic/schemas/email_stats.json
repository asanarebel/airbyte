    },
    "email_id": {
      "type": "integer"
    },
    "lead_id": {
      "type": "integer"
    },
    "ip_id": {
      "type": "integer"
    },
    "email_address": {
      "type": "string"
    },
    "date_sent": {
      "type": "string",
      "format": "date-time"
    },
    "is_read": {
      "type": "boolean"
    },
    "is_failed": {
      "type": "boolean"
    },
    "viewed_in_browser": {
      "type": "boolean"
    },
    "date_read": {
      "type": "string",
